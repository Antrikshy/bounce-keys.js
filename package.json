  },
  "devDependencies": {
    "jsdom": "^26.0.0",
    "minispec": "^1.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "author": "Antriksh Yadav",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Antrikshy/bounce-keys-js.git"
  }
